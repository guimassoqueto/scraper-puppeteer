{
  "name": "scraper-puppeteer",
  "version": "1.1.1",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "build": "tsc",
    "tsc:watch": "tsc --watch",
    "node:watch": "node --watch dist/amazon/main.js",
    "run:main": "ts-node src/amazon/main.ts",
    "lint:staged": "lint-staged",
    "test": "jest --passWithNoTests --runInBand",
    "test:simplified": "npm test -- --silent --noStackTrace --coverage=false",
    "test:unit": "npm test -- --noStackTrace --silent -c jest-unit.config.ts",
    "test:integration": "npm test -- --noStackTrace --silent -c jest-integration.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "puppeteer": "^20.4.0"
  }
}
