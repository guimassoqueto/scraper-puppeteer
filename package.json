{
  "name": "scraper-puppeteer",
  "version": "1.1.1",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "build": "tsc",
    "tsc:watch": "tsc --watch",
    "node:watch": "node --watch dist/main.js",
    "run:ts": "ts-node src/main.ts",
    "run:js": "node dist/main.js",
    "lint:staged": "lint-staged",
    "test": "jest --passWithNoTests --runInBand",
    "test:simplified": "npm test -- --silent --noStackTrace --coverage=false",
    "test:unit": "npm test -- --noStackTrace --silent -c jest-unit.config.ts",
    "test:integration": "npm test -- --noStackTrace --silent -c jest-integration.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/user-agents": "^1.0.2",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "puppeteer": "^20.7.3",
    "user-agents": "^1.0.1435"
  }
}
